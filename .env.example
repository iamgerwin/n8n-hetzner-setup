# N8N Configuration
# Update these with your actual domain and settings
N8N_HOST=n8n.example.com
N8N_PORT=5678
N8N_PROTOCOL=https
WEBHOOK_URL=https://n8n.example.com/
N8N_EDITOR_BASE_URL=https://n8n.example.com/
# Generate a secure encryption key: openssl rand -hex 32
N8N_ENCRYPTION_KEY=your_random_encryption_key_here_32_chars_hex

# PostgreSQL Configuration
# Change these passwords to secure, random values
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change_this_to_secure_password
POSTGRES_DB=n8n
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=change_this_to_secure_password

# Redis Configuration
# Change this password to a secure, random value
REDIS_PASSWORD=change_this_to_secure_password

# Execution Mode (Queue for worker scaling)
EXECUTIONS_MODE=queue
QUEUE_HEALTH_CHECK_ACTIVE=true
OFFLOAD_MANUAL_EXECUTIONS_TO_WORKERS=true

# Traefik Configuration (for domain routing)
TRAEFIK_DOMAIN=n8n.example.com
N8N_EXTERNAL_PORT=443

# Timezone
GENERIC_TIMEZONE=UTC

# Security
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=false
INSECURE_COOKIE=false
N8N_SECURE_COOKIE=true
